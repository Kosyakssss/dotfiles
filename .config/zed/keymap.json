[
  {
    "context": "(VimControl && !menu)",
    "bindings": {
      "j": [
        "vim::Down",
        {
          "display_lines": true,
        },
      ],
      "k": [
        "vim::Up",
        {
          "display_lines": true,
        },
      ],
    },
  },
  {
    "context": "Editor && vim_mode == insert",
    "bindings": {
      "j k": "vim::NormalBefore",
    },
  },
  {
    "context": "Editor",
    "bindings": {
      "alt-s": [
        "editor::SplitSelectionIntoLines",
        {
          "keep_selections": true,
        },
      ],
    },
  },
  {
    "context": "((vim_mode || vim_mode) && !menu)",
    "bindings": {
      "alt-shift-o": "editor::SelectLargerSyntaxNode",
    },
  },
  {
    "context": "((vim_mode || vim_mode ) && !menu)",
    "bindings": {
      "alt-shift-i": "editor::SelectSmallerSyntaxNode",
    },
  },
]
